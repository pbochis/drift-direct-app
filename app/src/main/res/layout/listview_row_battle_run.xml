<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout9">


    <FrameLayout
        android:layout_width="40dp"
        android:background="@drawable/battle_run_ended_shape"
        android:layout_height="40dp"
        android:id="@+id/frameLayout_battleRunBubbleListViewRow_endedBubble" />

    <FrameLayout
        android:layout_width="40dp"
        android:background="@drawable/battle_run_next_shape"
        android:id="@+id/frameLayout_battleRunBubbleListViewRow_nextBubble"
        android:layout_height="40dp" />

    <FrameLayout
        android:layout_width="40dp"
        android:background="@drawable/battle_run_current_shape"
        android:id="@+id/frameLayout_battleRunBubbleListViewRow_currentBubble"
        android:layout_height="40dp"/>

        <TextView
            android:layout_width="30dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/textView_battleRunBubbleListViewRow_runNumber"
            android:textSize="12sp"
            android:layout_centerInParent="true"
            android:layout_height="30dp"
            android:layout_gravity="center" />

    </RelativeLayout>


    <LinearLayout
        android:layout_width="30dp"
        android:layout_height="5dp"
        android:id="@+id/linearLayout_battleRunBubbleListViewRow_layoutConnector"
        android:background="@color/battle_run_ended_red"
        android:layout_alignBottom="@+id/relativeLayout9"
        android:layout_toRightOf="@+id/relativeLayout9"
        android:layout_toEndOf="@+id/relativeLayout9"
        android:layout_marginBottom="17dp" />


</RelativeLayout>